{
  "name": "@oselvar/noflare",
  "version": "1.1.2",
  "description": "Test helpers for Cloudflare Workers",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cloudflare",
    "testing"
  ],
  "homepage": "https://github.com/oselvar/noflare#readme",
  "bugs": {
    "url": "https://github.com/oselvar/noflare/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/oselvar/noflare.git"
  },
  "license": "MIT",
  "author": "Aslak Helles√∏y",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./test": {
      "types": "./dist/test/index.d.ts",
      "import": "./dist/test/index.js"
    },
    "./cloudflare": {
      "types": "./dist/cloudflare/index.d.ts",
      "import": "./dist/cloudflare/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "start": "wrangler dev --port 9875",
    "fix": "tsc && eslint --fix . && prettier --write .",
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "upgrade": "npm-check-updates --upgrade --deep"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20251115.0",
    "@eslint/compat": "2.0.0",
    "@eslint/js": "9.39.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "24.10.1",
    "@vitest/coverage-v8": "4.0.9",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "16.5.0",
    "np": "10.2.0",
    "npm-check-updates": "19.1.2",
    "prettier": "3.6.2",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.4",
    "vitest": "4.0.9",
    "wrangler": "4.47.0"
  }
}
